  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve",
    "startprod": "ng serve --prod",
    "startuat": "ng serve -c uat",
    "buildlocal": "ng build --prod --aot --named-chunks --build-optimizer --vendor-chunk --output-hashing all --extract-css true",
    "build": "ng build --prod --aot --named-chunks --build-optimizer --vendor-chunk --output-hashing all --extract-css true --base-href /Stork/StorkWeb/",
    "buildlive": "ng build --prod --aot --named-chunks --build-optimizer --vendor-chunk --output-hashing bundles --extract-css true --base-href /Stork/StorkWeb/",
    "buildstage": "ng build -c uat --aot --named-chunks --build-optimizer --vendor-chunk --output-hashing bundles --extract-css true --base-href /Stork/StorkWeb/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
